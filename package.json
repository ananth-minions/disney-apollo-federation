  "scripts": {
    "start": "concurrently -n \"ATTRACTIONS,DINING,HOTELS\" -c \"yellow,blue,green\" \"yarn start:attractions\" \"yarn start:dining\" \"yarn start:hotels\"",
    "start:dining": "cd dining && yarn dev",
    "start:attractions": "cd attractions && yarn dev",
    "start:hotels": "cd hotels && yarn dev"
  },
  "dependencies": {
    "concurrently": "^4.1.0"
  }
}
